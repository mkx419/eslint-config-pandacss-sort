{
  "name": "@mkx419/eslint-config-pandacss-sort",
  "version": "0.1.2",
  "description": "ESLint config for sorting Panda CSS",
  "keywords": [
    "eslint",
    "eslint-config",
    "pandacss",
    "sort"
  ],
  "homepage": "https://github.com/mkx419/eslint-config-pandacss-sort?tab=readme-ov-file#readme-ov-file",
  "bugs": {
    "url": "https://github.com/mkx419/eslint-config-pandacss-sort/issues",
    "email": "mkx419@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "mkx419",
    "email": "mkx419@gmail.com"
  },
  "type": "module",
  "files": [
    "./dist"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkx419/eslint-config-pandacss-sort.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format=cjs,esm --dts"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "pnpm": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "dependencies": {
    "@pandacss/config": "^0.45.2",
    "eslint-plugin-perfectionist": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.2",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  }
}
